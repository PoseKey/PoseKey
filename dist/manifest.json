{
    "manifest_version":2,
    "name": "Posenet Chrome extension",
    "description": "A chrome extension that connects your motion to browser action with the webcam",
    "version": "1.0",
    "permissions":[
        "activeTab",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "thumbs-up.png"
    }
}